              "$ref": "#/components/schemas/Heat"
            }
          },
          "id": {
            "type": "integer"
          }
        },
        "required": [
          "number"
        ]
      },
      "Meet": {
        "type": "object",
        "properties": {
          "current_start": {
            "type": "integer"
          },
          "name": {
            "type": "string",
            "maxLength": 255
          },
          "description": {
            "type": "string",
